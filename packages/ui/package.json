{
  "name": "@sandeep-jaiswar/ui",
  "version": "0.0.1",
  "description": "Complete Bloomberg Terminal-inspired React UI component library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "keywords": [
    "react",
    "ui",
    "components",
    "financial",
    "trading",
    "bloomberg",
    "terminal"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@sandeep-jaiswar/tokens": "workspace:*",
    "@sandeep-jaiswar/theme": "workspace:*",
    "@sandeep-jaiswar/utils": "workspace:*",
    "@sandeep-jaiswar/icons": "workspace:*",
    "@sandeep-jaiswar/button": "workspace:*",
    "@sandeep-jaiswar/input": "workspace:*",
    "@sandeep-jaiswar/badge": "workspace:*",
    "@sandeep-jaiswar/card": "workspace:*",
    "@sandeep-jaiswar/modal": "workspace:*",
    "@sandeep-jaiswar/navigation": "workspace:*",
    "@sandeep-jaiswar/data-grid": "workspace:*",
    "@sandeep-jaiswar/charts": "workspace:*",
    "@sandeep-jaiswar/trading": "workspace:*",
    "@sandeep-jaiswar/dashboard": "workspace:*"
  },
  "devDependencies": {
    "@sandeep-jaiswar/eslint-config": "workspace:*",
    "@sandeep-jaiswar/tsconfig": "workspace:*",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
